{
  "Radio": {
    "type": "NMEADevice",
    "device_io": {
      "type": "Serial",
      "port": "/dev/ttyUSB0",
      "encoding": "UTF-8"
    }
  },
  "AIS": {
    "type": "NMEADevice",
    "device_io": {
      "type": "Serial",
      "port": "/dev/ttyUSB3",
      "baudrate": 38400,
      "encoding": "UTF-8"
    }
  },
  "GPS": {
    "type": "NMEADevice",
    "auto_flush": 10,
    "device_io": {
      "type": "Serial",
      "port": "/dev/ttyUSB1",
      "encoding": "ASCII"
    }
  },
  "Wind": {
    "type": "NMEADevice",
    "device_io": {
      "type": "Serial",
      "port": "/dev/ttyUSB2",
      "encoding": "ASCII"
    }
  },
  "Seatalk": {
    "type": "SeatalkDevice",
    "device_io": {
      "type": "SeatalkSerial",
      "port": "/dev/ttyAMA0",
      "encoding": false
    }
  },
  "TCP": {
    "type": "NMEADevice",
    "device_io": {
      "type": "TCPServer",
      "port": "9900",
      "encoding": "UTF-8"
    }
  },
  "TimeSetter": {
    "type": "SetTimeDevice",
    "device_io": {
      "type": "IO"
    }
  }
}
