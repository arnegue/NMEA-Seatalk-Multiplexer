{
  "Radio": {
    "type": "NMEADevice",
    "device_io": {
      "type": "Serial",
      "port": "/dev/ttyUSB0",
      "encoding": "UTF-8"
    },
    "observers": [
      "TCP"
    ]
  },
  "AIS": {
    "type": "NMEADevice",
    "device_io": {
      "type": "Serial",
      "port": "/dev/ttyS2",
      "baudrate": 38400,
      "encoding": "UTF-8"
    },
    "observers": [
      "TCP"
    ]
  },
  "GPS": {
    "type": "NMEADevice",
    "auto_flush": 10,
    "device_io": {
      "type": "Serial",
      "port": "/dev/ttyUSB1",
      "encoding": "ASCII"
    },
    "observers": [
      "TCP"
    ]
  },
  "Wind": {
    "type": "NMEADevice",
    "device_io": {
      "type": "Serial",
      "port": "/dev/ttyUSB2",
      "encoding": "ASCII"
    },
    "observers": [
      "TCP"
    ]
  },
  "Seatalk": {
    "type": "SeatalkDevice",
    "device_io": {
      "type": "Serial",
      "port": "/dev/ttyUSB3",
      "parity": "Mark",
      "encoding": false
    },
    "observers": [
      "TCP"
    ]
  },
  "TCP": {
    "type": "NMEADevice",
    "device_io": {
      "type": "TCPServer",
      "port": "9900",
      "encoding": "UTF-8"
    },
    "observers": [
    ]
  }
